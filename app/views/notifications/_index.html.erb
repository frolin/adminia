<% current_user.notifications.each do |notice| %>

  <li>
    <div class="dropdown-messages-box">
      <a href="#" class="pull-left">
        <img alt="image" class="img-circle" src="<%= image_path('a7.jpg') %>"/>
      </a>
      <div class="media-body">
                          <small class="pull-right">notice.time_ago</small>
<!--        <strong></strong>-->
        <strong>  <%= link_to notice.to_notification.message, issue_path(notice[:params][:issue][:id]) %></strong>. <br/>
                          <small class="text-muted">3 days ago at 7:58 pm - 10.06.2014</small>
      </div>
    </div>
  </li>
<% end %>